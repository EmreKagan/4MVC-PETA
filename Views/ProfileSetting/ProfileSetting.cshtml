@{
    ViewData["Title"] = "ProfileSetting";

}
<div class="content-wrapper">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Şifre Değiştir</h4>
                
                <form class="forms-sample">
             
                    <div class="form-group">
                        <label for="exampleInputPassword1">Şifre</label>
                        <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Şifre">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputConfirmPassword1">Yeni Şifre </label>
                        <input type="password" class="form-control" id="exampleInputConfirmPassword1" placeholder="Yeni Şifre">
                    </div>
                    
                    <button type="submit" class="btn btn-primary mr-2">Kaydet</button>
                    <button class="btn btn-dark">Vazgeç</button>
                </form>
            </div>
        </div>
    </div>

</div>